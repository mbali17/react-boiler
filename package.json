{
  "name": "kmireactboiler",
  "version": "19.1.0",
  "description": "KMi React Boiler",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack -p --optimize-minimize --env.NODE_ENV=production",
    "flow-check": "flow check",
    "flow-coverage": "flow-coverage-report --threshold 92 -i 'src/**/*.js' -i 'src/**/*.jsx' --collectCoverageFrom=!src/styled/**.* -t html -t json -t text",
    "lint": "eslint --ext .js --ext .jsx src test scripts",
    "start": "./scripts/start",
    "start:client-dev": "webpack serve --mode development --open 'google-chrome' --hot",
    "server:start": "node ./server/server.js",
    "server:watch": "babel src --ignore src/client --watch --out-dir dist --copy-files --source-maps inline",
    "test": "./scripts/test --env.NODE_ENV=test",
    "test-suite": "jest test/*/unit test/*/integration --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "globals": {
      "window": true,
      "document": true
    },
    "setupFiles": [
      "<rootDir>/test/jest_setup.js",
      "<rootDir>/test/jest_shim.js"
    ],
    "testURL": "http://localhost:8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://AvalaraDev@github.com/AvalaraDev/MtgAvaFormReact.git"
  },
  "keywords": [
    "Webpack"
  ],
  "author": "khippert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AvalaraDev/MtgAvaFormReact/issues"
  },
  "homepage": "https://github.com/AvalaraDev/MtgAvaFormReact#readme",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@babel/register": "^7.11.5",
    "ajv": "^7.0.2",
    "autoprefixer": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-async-helpers": "^0.9.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flatpickr": "^4.6.6",
    "html-webpack-plugin": "^4.4.1",
    "isomorphic-fetch": "^3.0.0",
    "json-loader": "^0.5.7",
    "moment": "^2.27.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flatpickr": "^3.10.6",
    "react-intl-phone3": "^0.3.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.0",
    "webpack": "^5.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "compression-webpack-plugin": "^7.1.0",
    "css-loader": "^5.0.1",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "flow-bin": "^0.141.0",
    "flow-coverage-report": "^0.8.0",
    "flow-typed": "^3.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-mock-console": "^1.0.1",
    "jest-styled-components": "^7.0.3",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
